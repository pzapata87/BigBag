<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Banco Falabella - @ViewBag.Title</title>

    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">

    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")" />

    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/font-awesome.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.gritter.css")" />

    <!-- text fonts -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/ace-fonts.css")" />

    <!-- ace styles -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/ace.css")"/>

    <!-- inline styles related to this page -->
    @RenderSection("styles", required: false)

    <!-- The main stylesheet of this template. All CSS overwrites are defined in here -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/custom.css")" type="text/css" />
</head>
<body class="no-skin">
    <div id="navbar" class="navbar navbar-default">
        <script type="text/javascript">
            try { ace.settings.check('navbar', 'fixed') } catch (e) { }
        </script>
        <div class="navbar-container" id="navbar-container">
            <!-- #section:basics/sidebar.mobile.toggle -->
            <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler" data-target="#sidebar">
                <span class="sr-only">Toggle sidebar</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <!-- /section:basics/sidebar.mobile.toggle -->
            <div class="navbar-header pull-left">
                <!-- #section:basics/navbar.layout.brand -->
                <a href="#" class="navbar-brand">
                    @*<img src="@Url.Content("~/Content/images/banco-falabella.jpg")" alt="" class="img-responsive" width="150">*@
                </a>
            </div>
            <!-- #section:basics/navbar.dropdown -->
            <div class="navbar-buttons navbar-header pull-right" role="navigation">
                <ul class="nav ace-nav">
                    <!-- #section:basics/navbar.user_menu -->
                    <li class="green">
                        <a data-toggle="dropdown"
                           href="javascript:void(0)" class="dropdown-toggle">
                            <img class="nav-user-photo" src="@Url.Content("~/Content/images/avatar2.png")" alt="User's Photo" />
                            <span class="user-info">
                                <small>Bienvenido,</small>
                                @WebSession.Usuario.NombreCompleto
                            </span> <i class="ace-icon fa fa-caret-down"></i>
                        </a>
                        <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                            <li>
                                <a href="@Url.Action("Logout", "Account")">
                                    <i class="ace-icon fa fa-power-off"></i> @Master.CerrarSesion
                                </a>
                            </li>
                        </ul>
                    </li>
                    <!-- /section:basics/navbar.user_menu -->
                </ul>
            </div>
            <!-- /section:basics/navbar.dropdown -->
        </div><!-- /.navbar-container -->
    </div>

    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>
        <!-- #section:basics/sidebar -->
        <div id="sidebar" class="sidebar responsive menu-min">
            <script type="text/javascript">
                try { ace.settings.check('sidebar', 'fixed') } catch (e) { }
            </script>

            <div class="sidebar-shortcuts" id="sidebar-shortcuts">
                <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
                    <span class="btn btn-successFalabella"></span>
                    <span class="btn btn-successFalabella"></span>
                    <span class="btn btn-successFalabella"></span>
                    <span class="btn btn-successFalabella"></span>
                </div>
            </div><!-- /.sidebar-shortcuts -->
            <ul id="ulMenu" class="nav nav-list">
                <li class="">
                    <a href="/">
                        <i class="menu-icon fa fa-tachometer"></i>
                        <span class="menu-text"> Dashboard </span>
                    </a>
                    <b class="arrow"></b>
                </li>
                <li class="">
                    <a href="#" class="dropdown-toggle">
                        <i class="menu-icon fa fa-users"></i>
                        <span class="menu-text"> Seguridad </span>
                        <b class="arrow fa fa-angle-down"></b>
                    </a>
                    <b class="arrow"></b>
                    <ul class="submenu">
                        <li class="">
                            <a href="@Url.Action("Index", "Usuario")">
                                <i class="menu-icon fa fa-caret-right"></i>
                                Usuarios
                            </a>
                            <b class="arrow"></b>
                        </li>
                    </ul>
                </li>
            </ul><!-- /.nav-list -->
            <!-- #section:basics/sidebar.layout.minimize -->
            <div class="sidebar-toggle sidebar-collapse" id="sidebar-collapse">
                <i class="ace-icon fa fa-angle-double-left" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i>
            </div>

            <!-- /section:basics/sidebar.layout.minimize -->
            <script type="text/javascript">
                try { ace.settings.check('sidebar', 'collapsed') } catch (e) { }
            </script>
        </div>

        <!-- /section:basics/sidebar -->
        <div class="main-content">
            <div class="main-content-inner">
                <!-- #section:basics/content.breadcrumbs -->
                <div class="breadcrumbs" id="breadcrumbs">
                    <script type="text/javascript">
                        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
                    </script>

                    <ul class="breadcrumb">
                        <li>
                            <i class="ace-icon fa fa-home home-icon"></i>
                            <a href="/">Home</a>
                        </li>
                        <li>
                            <a id="link2" href="javascript:void(0)"></a>
                        </li>
                        <li id="link3" class="active"></li>
                    </ul><!-- /.breadcrumb -->
                </div>
                <!-- /section:basics/content.breadcrumbs -->
                <div class="page-content">
                    <div class="row">
                        <div class="col-xs-12">
                            <!-- PAGE CONTENT BEGINS -->
                            @RenderBody()
                            <!-- PAGE CONTENT ENDS -->
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.page-content -->
            </div>
        </div><!-- /.main-content -->
        <div class="footer">
            <div class="footer-inner">
                <!-- #section:basics/footer -->
                <div class="footer-content">
                    <span class="sigcomt">
                        Copyright &copy; 2017 Sigcomt SAC. Todos los derechos reservados
                    </span>
                </div>
                <!-- /section:basics/footer -->
            </div>
        </div>
    </div><!-- /.main-container -->
    <!-- basic scripts -->

    <!--[if !IE]> -->
    <script type="text/javascript">
        window.jQuery || document.write("<script src='@Url.Content("~/Content/js/jquery.js")'>" + "<" + "/script>");
    </script>
    <!-- <![endif]-->

    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='@Url.Content("~/Content/js/jquery.mobile.custom.js")'>" + "<" + "/script>");
    </script>

    <!-- Bootstrap.js -->
    <script src="@Url.Content("~/Content/js/bootstrap.js")" type="text/javascript"></script>

    <!-- ace scripts -->
    <script src="@Url.Content("~/Content/js/ace/elements.scroller.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/ace/ace.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/ace/ace.sidebar.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/ace/ace.sidebar-scroll-1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/ace/ace.submenu-hover.js")" type="text/javascript"></script>

    <!-- Jquery plugins and custom javascript code -->
    <script src="@Url.Content("~/Content/js/jquery.gritter.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.blockUI.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/webApp.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/viewsJs/main.js")" type="text/javascript"></script>

    <script type="text/javascript"> ace.vars['base'] = '..'; </script>

    <!-- inline scripts related to this page -->
    <script type="text/javascript">
        $(function() {
            var parametros = {
                mensajeEspera: 'Espere un momento por favor...',
                tituloPopupMensaje: 'Informaci&oacute;n',
                tituloPopupComfirmacion: 'Confirmaci&oacute;n',
                mensajePopupConfirmacion: 'Seguro de continuar con este proceso?',
                btnCancelar: 'Cancelar',
                btnAceptar: 'Aceptar',
                formatoFecha: 'dd/MM/yyyy hh:mm'
            };

            webApp.init(parametros);
            mainJs.init();
        });
    </script>

    <!-- Javascript code only for this page -->
    @RenderSection("scripts", required: false)
</body>
</html>