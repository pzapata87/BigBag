<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Banco Falabella - Cobranzas</title>

    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">

    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/Content/images/favicon.ico")" />

    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/font-awesome.css")"/>
    <link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.gritter.css")" />

    <!-- text fonts -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/ace-fonts.css")" />

    <!-- ace styles -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/ace.css")" />

    <!-- The main stylesheet of this template. All CSS overwrites are defined in here -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/custom.css")" type="text/css" />
</head>

<body class="login-layout">
    <div class="main-container">
        <div class="main-content">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1">
                    <!-- PAGE CONTENT BEGINS -->
                    @RenderBody()
                    <!-- PAGE CONTENT ENDS -->
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.main-content -->
    </div><!-- /.main-container -->

    <spring:url value="/resources/js/jquery.js" var="jqueryJs" />
    <spring:url value="/resources/js/jquery.mobile.custom.js" var="jqueryMobileCustomJs" />
    <spring:url value="/resources/js/jquery.gritter.js" var="jqueryGritterJs" />
    <spring:url value="/resources/js/jquery.blockUI.js" var="jqueryBlockUIJs" />
    <spring:url value="/resources/js/bootstrap.js" var="bootstrapJs" />

    <spring:url value="/resources/js/ace/elements.scroller.js" var="scrollerJs" />
    <spring:url value="/resources/js/ace/ace.js" var="aceJs" />

    <spring:url value="/resources/js/webApp.js" var="webAppJs" />
    <spring:url value="/resources/js/viewsJs/login.js" var="loginJs" />

    <!--[if !IE]> -->
    <script type="text/javascript">
        window.jQuery || document.write("<script src='@Url.Content("~/Content/js/jquery.js")'>" + "<" + "/script>");
    </script>
    <!-- <![endif]-->

    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='@Url.Content("~/Content/js/jquery.mobile.custom.js")'>" + "<" + "/script>");
    </script>

    <!-- Bootstrap.js -->
    <script src="@Url.Content("~/Content/js/bootstrap.js")" type="text/javascript"></script>

    <!-- ace scripts -->
    <script src="@Url.Content("~/Content/js/ace/elements.scroller.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/ace/ace.js")" type="text/javascript"></script>

    <!-- Jquery plugins and custom javascript code -->
    <script src="@Url.Content("~/Content/js/jquery.gritter.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.blockUI.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/webApp.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/viewsJs/login.js")" type="text/javascript"></script>

    <script type="text/javascript"> ace.vars['base'] = '..'; </script>

    <script type="text/javascript">
	    $(function () {
	    	loginJs.init();
	    });
    </script>
</body>
</html>